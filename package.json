{
  "name": "kenshi",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "npm run docs:build && npm run blog:build && vite build",
    "preview": "vite preview",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "docs:watch": "chokidar \"src/routes/docs/**/*.cadey\" -c \"node docs.js\"",
    "docs:build": "node docs.js",
    "blog:watch": "chokidar \"src/routes/blog/**/*.cadey\" -c \"node blog.js\"",
    "blog:build": "node blog.js"
  },
  "devDependencies": {
    "@zerodevx/svelte-toast": "^0.6.3",
    "bn.js": "^5.2.1",
    "buffer": "^6.0.3",
    "chokidar-cli": "^3.0.0",
    "cli-progress": "^3.11.2",
    "crypto-js": "^4.1.1",
    "elliptic": "^6.5.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "jsdom": "^20.0.1",
    "layercake": "^6.1.0",
    "ora": "^6.1.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "rollup-plugin-polyfill-node": "^0.8.0",
    "svelte": "^3.55.0",
    "svelte-loading-spinners": "^0.1.7"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.53.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@web3-onboard/core": "^2.14.1",
    "@web3-onboard/injected-wallets": "^2.6.2",
    "@web3-onboard/uauth": "^2.0.1",
    "@web3-onboard/walletconnect": "^2.2.2",
    "bnc-onboard": "^1.39.1",
    "cadey": "^1.0.5",
    "carbon-components-svelte": "^0.72.2",
    "carbon-icons-svelte": "^11.4.0",
    "carbon-pictograms-svelte": "^12.2.0",
    "clipboard-copy": "^4.0.1",
    "d3-shape": "^3.1.0",
    "dotenv": "^10.0.0",
    "ethers": "^5.5.1",
    "format-thousands": "^2.0.0",
    "mongodb": "^4.2.1",
    "percentile": "^1.6.0",
    "pygments": "^0.2.1",
    "rxjs": "^7.5.7",
    "shave": "^5.0.2",
    "svelte-simple-modal": "^1.4.1",
    "svelte-skeleton": "^1.3.1",
    "vite": "^4.1.1"
  }
